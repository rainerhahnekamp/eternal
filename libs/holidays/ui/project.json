{
  "name": "holidays-ui",
  "projectType": "library",
  "sourceRoot": "libs/holidays/ui/src",
  "prefix": "eternal",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/holidays/ui"],
      "options": {
        "jestConfig": "libs/holidays/ui/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/holidays/ui/**/*.ts",
          "libs/holidays/ui/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:holidays", "type:ui"]
}
