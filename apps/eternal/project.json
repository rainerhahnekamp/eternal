{
  "name": "eternal",
  "projectType": "application",
  "sourceRoot": "apps/eternal/src",
  "prefix": "eternal",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/eternal",
        "index": "apps/eternal/src/index.html",
        "main": "apps/eternal/src/main.ts",
        "polyfills": "apps/eternal/src/polyfills.ts",
        "tsConfig": "apps/eternal/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/eternal/src/favicon.ico", "apps/eternal/src/assets"],
        "styles": ["apps/eternal/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/eternal/src/environments/environment.ts",
              "with": "apps/eternal/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        },
        "dev-local": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/eternal/src/environments/environment.ts",
              "with": "apps/eternal/src/environments/environment.dev-local.ts"
            }
          ]
        },
        "non-mock": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/eternal/src/environments/environment.ts",
              "with": "apps/eternal/src/environments/environment.non-mock.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "eternal:build:production"
        },
        "development": {
          "browserTarget": "eternal:build:development"
        },
        "dev-local": {
          "browserTarget": "eternal:build:dev-local"
        },
        "non-mock": {
          "browserTarget": "eternal:build:non-mock"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "eternal:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/eternal/**/*.ts", "apps/eternal/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/eternal"],
      "options": {
        "jestConfig": "apps/eternal/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "apps/eternal/.storybook",
        "browserTarget": "eternal:build",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputDir": "dist/storybook/eternal",
        "configDir": "apps/eternal/.storybook",
        "browserTarget": "eternal:build",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "component-test": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/eternal/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "eternal:build"
      }
    }
  },
  "tags": []
}
